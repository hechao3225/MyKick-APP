<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"

    android:id="@+id/layout"
    android:keepScreenOn="true"
    tools:context=".MyKicklj23" >


    <ImageView
        android:id="@+id/mouse3"
        android:layout_width="82dp"
        android:layout_height="82dp"
        android:src="@drawable/mouse3"
        android:layout_gravity="bottom" />


    <ImageView
        android:id="@+id/add50"
        android:layout_width="82dp"
        android:layout_height="82dp"
        android:src="@drawable/add50"
        android:layout_gravity="center_horizontal|top" />

    <ImageView
        android:id="@+id/add30"
        android:layout_width="68dp"
        android:layout_height="68dp"
        android:src="@drawable/add30"
        android:layout_gravity="center" />

    <ImageView
        android:id="@+id/add20"
        android:layout_width="68dp"
        android:layout_height="68dp"
        android:src="@drawable/add20"
        android:layout_gravity="right|center_vertical" />

    <ImageView
        android:id="@+id/add10"
        android:layout_width="67dp"
        android:layout_height="67dp"
        android:src="@drawable/add10"
        android:layout_gravity="left|center_vertical" />

    <ImageView
        android:id="@+id/mouse_boss"
        android:layout_width="match_parent"
        android:layout_height="98dp"
        android:src="@drawable/mouse_boss"
        android:layout_gravity="right|bottom" />

    <ImageView
        android:id="@+id/mouse1"
        android:layout_width="89dp"
        android:layout_height="wrap_content"
        android:src="@drawable/mouse1"
        android:layout_gravity="left|bottom" />

    <ImageView
        android:id="@+id/mouse_not"
        android:layout_width="82dp"
        android:layout_height="82dp"
        android:src="@drawable/mouse_not"
        android:layout_gravity="right|bottom" />

    <ImageView
        android:id="@+id/mouse2"
        android:layout_width="82dp"
        android:layout_height="82dp"
        android:src="@drawable/mouse2"
        android:layout_gravity="right|bottom" />

    <Chronometer
        android:id="@+id/chronometer1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:textSize="3mm"
        android:textColor="#ff704d"
        android:text="Chronometer"
        android:layout_gravity="right|top" />

    <ImageView
        android:id="@+id/sub50"
        android:layout_width="82dp"
        android:layout_height="82dp"
        android:src="@drawable/sub50"
        android:layout_gravity="left|top" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:textSize="3mm"
        android:textColor="#ff004d"
        android:text="积分：00"
        android:layout_gravity="left|top" />

    <Chronometer
        android:id="@+id/chronometer2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:textSize="3mm"
        android:textColor="#ff704d"
        android:text="Chronometer"
        android:layout_gravity="center_horizontal|top" />


</FrameLayout>
